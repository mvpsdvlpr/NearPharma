{
  "version": 2,
  "builds": [
    { "src": "src/app.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/mfarmacias/(.*)", "dest": "/backend/src/app.ts" },
    { "src": "/mfarmacias/mapa.php", "dest": "/backend/src/app.ts" },
    { "src": "/api/(.*)", "dest": "/backend/src/app.ts" }
  ]
}
